<?xml version="1.0"?>

<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" launch-prefix="xterm -e" >
    <rosparam file="$(find rt2_robot)/param/costmap_common_params.yaml" command="load" ns="global_costmap"/>
    <rosparam file="$(find rt2_robot)/param/costmap_common_params.yaml" command="load" ns="local_costmap"/>
    <rosparam file="$(find rt2_robot)/param/local_costmap_params.yaml" command="load"/>
    <rosparam file="$(find rt2_robot)/param/global_costmap_params.yaml" command="load"/>
    <rosparam file="$(find rt2_robot)/param/base_local_planner_params.yaml" command="load"/>
    <rosparam file="$(find rt2_robot)/param/move_base_params.yaml" command="load"/>
  </node>
</launch>